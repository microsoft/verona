class I32 {}
class I64 {}
class F32 {}
class F64 {}

simple(a: I32, b: I32): I32
{
  let x : I32 = a + b;
  return x;
}

int_upcast(a: I32, b: I64): I64
{
  let x : I64 = a + b;
  return x;
}

fp_upcast(a: F64, b: F32): F64
{
  let x : F64 = a + b;
  return x;
}

literals(): F64
{
  let x : I32 = 21;
  let y : I64 = x + 21;
  let f : F32 = 3.14;
  let d : F64 = f + 0.0015;
  return d;
}
