f(x : U32) : U32
{
  if (x < 2)
  {
    x = x + 1;
  }
  else if (x > 100)
  {
    x = x - 1;
  }
  else if (x <= 20)
  {
    x = x * 2;
  }
  else if (x >= 50)
  {
    x = x / 10;
  }
  else if (x == 10)
  {
    x = 10;
  }
  else if (x != 20)
  {
    x = 42;
  }
  else
  {
    x = 0;
  }
  x;
}
